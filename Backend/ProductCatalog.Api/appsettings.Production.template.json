{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host={POSTGRES_HOST};Port=5432;Database=ProductCatalogDb;Username={POSTGRES_USER}@{POSTGRES_SERVER};Password={POSTGRES_PASSWORD};SSL Mode=Require;Include Error Detail=true;"
  },
  "Jwt": {
    "Key": "{JWT_KEY}",
    "Issuer": "ProductCatalogApi",
    "Audience": "ProductCatalogClient",
    "ExpirationHours": "24"
  },
  "Cors": {
    "AllowedOrigins": "https://app-productcatalog-web.azurewebsites.net,https://localhost:5001,https://localhost:3000"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "{INSTRUMENTATION_KEY}"
  }
}
